{
    "name": "sloth-dev-guy/mvc-enhancements",
    "description": "MVC Enhancements for your laravel or lumen projects",
    "keywords": [
        "laravel",
        "library",
        "lumen",
        "mvc"
    ],
    "homepage": "https://github.com/sloth-dev-guy/mvc-enhancements",
    "authors": [
        {
            "name": "Telmo Rafael",
            "email": "sloth.dev.guy@gmail.com",
            "homepage": "https://sloth-guy.dev"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^9.0",
        "illuminate/container": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/events": "^9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "jenssegers/mongodb": "^3.9",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.3",
        "doctrine/dbal": "^2.13|^3.1",
        "vlucas/phpdotenv": "^5.4",
        "laravel/lumen-framework": "^9.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "SlothDevGuy\\MVCEnhancements\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "laravel/lumen-framework": "Required to use defaults functionalities inherit to the laravel frameworks (laravel|lumen) (^9.0)",
        "illuminate/http": "Required to use custom requests object in controllers (^9.0)",
        "jenssegers/mongodb": "Required for mongodb eloquent adapter (^3.9)",
        "ext-mongodb": "Required for mongodb driver support (^1.13.0)"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SlothDevGuy\\MVCEnhancements\\MVCEnhancementsServiceProvider"
            ]
        }
    }
}
